{
  "name": "Fortress",
  "type": "light",
  "colors": {
    "editor.background": "#f5f5f5",
    "editor.foreground": "#333333",
    "list.activeSelectionIconForeground": "#fff"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#AAAAAA"
      }
    },
    {
      "name": "Quotes",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#0090c9"
      }
    },
    {
      "name": "Constants obo Numbers",
      "scope": ["constant"],
      "settings": {
        "foreground": "#0090c9"
      }
    },
    {
      "name": "Variable",
      "scope": ["storage"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": ["support.type"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Braces",
      "scope": ["meta.brace"],
      "settings": {
        "foreground": "#000"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#333",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Tag",
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Tag Attribute name",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#002c8a"
      }
    },

    {
      "name": "Braces for JSX Variables",
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#0090c9"
      }
    },

    {
      "name": "Template-DO-NOT-DELETE",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#888",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Class names",
      "scope": ["entity.name.type"],
      "settings": {
        "fontStyle": "bold"
      }
    },

    {
      "name": "Template-DO-NOT-DELETE",
      "scope": ["paste_scope_here"],
      "settings": {
        "foreground": "#FF00FF"
      }
    }
  ]
}
